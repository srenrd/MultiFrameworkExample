using System;

public partial class _Default : System.Web.UI.Page
{
	protected void Page_Load(object sender, EventArgs e)
	{
		string v = Request.QueryString["v"];

		if (!string.IsNullOrWhiteSpace(v))
		{
			string raw;

			try
			{
				raw = Encryption.XorDecrypt(v);
			}
			catch
			{
				raw = null;
			}

			if (!string.IsNullOrWhiteSpace(raw) && raw.StartsWith("paymentId=", StringComparison.OrdinalIgnoreCase))
			{
                Response.Redirect("~/Payment/InitOrder.ashx?paymentId=" + raw.Substring(10) + "&culture=" + Request.QueryString["culture"], false);
				return;
			}
		}
	}
}